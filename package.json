{
  "name": "opencode-webhooks",
  "version": "0.0.1",
  "description": "Opencode plugin for sending webhook notifications on any Opencode event",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "opencode-webhooks": "./bin/run.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:cli": "tsc --project tsconfig.cli.json",
    "watch": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "package": "npm run build && npm pack",
    "prepublishOnly": "npm run build && npm run build:cli",
    "install-plugin": "tsx ./src/commands/install.ts"
  },
  "oclif": {
    "bin": "opencode-webhooks",
    "dirname": "opencode-webhooks",
    "commands": "./dist/commands",
    "topicSeparator": " "
  },
  "keywords": [
    "opencode",
    "plugin",
    "webhook",
    "notifications",
    "slack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@inquirer/prompts": "^8.0.1",
    "@oclif/core": "^4.8.0",
    "@opencode-ai/plugin": "^1.0.81",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "bun-types": "^1.3.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.0.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "peerDependencies": {
    "opencode": "*"
  },
  "peerDependenciesMeta": {
    "opencode": {
      "optional": true
    }
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:@zkochan/inflight@^2.0.0",
    "test-exclude": "^7.0.1",
    "babel-plugin-istanbul": "^7.0.0"
  }
}
