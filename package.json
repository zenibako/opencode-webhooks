{
  "name": "@opencode/webhook-plugin",
  "version": "0.0.1",
  "description": "Opencode plugin for sending webhook notifications on any Opencode event",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "package": "npm run build && npm pack",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "opencode",
    "plugin",
    "webhook",
    "notifications",
    "slack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "peerDependencies": {
    "opencode": "*"
  },
  "peerDependenciesMeta": {
    "opencode": {
      "optional": true
    }
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:@zkochan/inflight@^2.0.0",
    "test-exclude": "^7.0.1",
    "babel-plugin-istanbul": "^7.0.0"
  }
}
