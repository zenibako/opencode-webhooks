{
  "name": "opencode-webhooks",
  "version": "0.0.6",
  "description": "Opencode plugin for sending webhook notifications on any Opencode event",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:bun": "bun test tests/integration.bun.test.ts",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix"
  },
  "keywords": [
    "opencode",
    "plugin",
    "webhook",
    "notifications",
    "slack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenibako/opencode-webhooks.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@opencode-ai/plugin": "^1.0.81",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "bun-types": "^1.3.2",
    "eslint": "^9.0.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": "^1.0.0",
    "opencode": "*"
  },
  "peerDependenciesMeta": {
    "@opencode-ai/plugin": {
      "optional": true
    },
    "opencode": {
      "optional": true
    }
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:@zkochan/inflight@^2.0.0",
    "test-exclude": "^7.0.1",
    "babel-plugin-istanbul": "^7.0.0"
  }
}
